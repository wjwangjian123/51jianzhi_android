<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    tools:context=".mvp.ui.activity.RankActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/dimen_10">
        <RelativeLayout
            android:id="@+id/one_rl"
            android:layout_width="119dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20"
            android:background="@drawable/icon_choice_top_two"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="32dp"
            android:layout_toLeftOf="@+id/two_rl">
            <TextView
                android:id="@+id/one_tv_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:lines="2"
                android:lineSpacingExtra="@dimen/dimen_3"
                android:textColor="@color/color_333333"
                android:textSize="@dimen/font_10"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/one_tv_method"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/one_tv_title"
                android:layout_marginTop="@dimen/dimen_10"
                android:textColor="@color/color_666666"
                android:textSize="7sp" />

            <View
                android:id="@+id/one_view1"
                android:layout_width="0.5dp"
                android:layout_height="@dimen/dimen_5"
                android:layout_alignTop="@+id/one_tv_method"
                android:layout_marginLeft="@dimen/dimen_2"
                android:layout_marginTop="@dimen/dimen_2"
                android:layout_toRightOf="@+id/one_tv_method"
                android:background="@color/color_666666" />
            <TextView
                android:id="@+id/one_tv_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_666666"
                android:textSize="7sp"
                android:layout_alignTop="@+id/one_tv_method"
                android:layout_marginLeft="@dimen/dimen_2"
                android:layout_toRightOf="@+id/one_view1"/>
            <View
                android:id="@+id/one_view2"
                android:layout_width="0.5dp"
                android:layout_height="@dimen/dimen_5"
                android:layout_alignTop="@+id/one_tv_method"
                android:layout_marginLeft="@dimen/dimen_2"
                android:layout_marginTop="@dimen/dimen_2"
                android:layout_toRightOf="@+id/one_tv_time"
                android:background="@color/color_666666" />
            <TextView
                android:id="@+id/one_tv_sex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dimen_2"
                android:layout_toRightOf="@+id/one_view2"
                android:textColor="@color/color_666666"
                android:textSize="7sp"
                android:layout_alignTop="@+id/one_tv_method"/>
            <ImageView
                android:id="@+id/one_iv_score"
                android:layout_width="22dp"
                android:layout_height="13dp"
                android:layout_below="@+id/one_tv_method"
                android:layout_marginTop="17dp"
                android:src="@drawable/icon_choice_two"/>
            <TextView
                android:id="@+id/one_tv_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_666666"
                android:textSize="@dimen/font_8"
                android:gravity="right"
                android:layout_toRightOf="@+id/one_iv_score"
                android:layout_marginLeft="@dimen/dimen_3"
                android:layout_alignBottom="@+id/one_iv_score"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/two_rl"
            android:layout_width="119dp"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="45dp"
            android:layout_marginTop="@dimen/dimen_1"
            android:background="@drawable/icon_choice_top_one"
            android:layout_centerHorizontal="true">
            <TextView
                android:id="@+id/two_tv_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_333333"
                android:textSize="@dimen/font_10"
                android:textStyle="bold"
                android:lines="2"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/dimen_3"/>
            <TextView
                android:id="@+id/two_tv_method"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_666666"
                android:textSize="7sp"
                android:layout_below="@+id/two_tv_title"
                android:layout_marginTop="@dimen/dimen_10"/>
            <View
                android:id="@+id/two_view1"
                android:layout_width="0.5dp"
                android:layout_height="@dimen/dimen_5"
                android:background="@color/color_666666"
                android:layout_alignTop="@+id/two_tv_method"
                android:layout_toRightOf="@+id/two_tv_method"
                android:layout_marginLeft="@dimen/dimen_2"
                android:layout_marginTop="@dimen/dimen_2"/>
            <TextView
                android:id="@+id/two_tv_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_666666"
                android:textSize="7sp"
                android:layout_alignTop="@+id/two_tv_method"
                android:layout_toRightOf="@+id/two_view1"
                android:layout_marginLeft="@dimen/dimen_2"/>
            <View
                android:id="@+id/two_view2"
                android:layout_width="0.5dp"
                android:layout_height="@dimen/dimen_5"
                android:background="@color/color_666666"
                android:layout_alignTop="@+id/two_tv_method"
                android:layout_toRightOf="@+id/two_tv_time"
                android:layout_marginLeft="@dimen/dimen_2"
                android:layout_marginTop="@dimen/dimen_2"/>
            <TextView
                android:id="@+id/two_tv_sex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_666666"
                android:textSize="7sp"
                android:layout_alignTop="@+id/two_tv_method"
                android:layout_toRightOf="@+id/two_view2"
                android:layout_marginLeft="@dimen/dimen_2"/>
            <ImageView
                android:id="@+id/two_iv_score"
                android:layout_width="22dp"
                android:layout_height="13dp"
                android:layout_below="@+id/two_tv_method"
                android:layout_marginTop="17dp"
                android:src="@drawable/icon_choice_one"/>
            <TextView
                android:id="@+id/two_tv_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_8"
                android:textColor="@color/color_666666"
                android:gravity="right"
                android:layout_toRightOf="@+id/two_iv_score"
                android:layout_marginLeft="@dimen/dimen_3"
                android:layout_alignBottom="@+id/two_iv_score"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/three_rl"
            android:layout_width="119dp"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="32dp"
            android:layout_marginTop="@dimen/dimen_20"
            android:background="@drawable/icon_choice_top_three"
            android:layout_toRightOf="@+id/two_rl">
            <TextView
                android:id="@+id/three_tv_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_333333"
                android:textSize="@dimen/font_10"
                android:textStyle="bold"
                android:lines="2"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/dimen_3"/>
            <TextView
                android:id="@+id/three_tv_method"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_666666"
                android:textSize="7sp"
                android:layout_below="@+id/three_tv_title"
                android:layout_marginTop="@dimen/dimen_10"/>
            <View
                android:id="@+id/three_view1"
                android:layout_width="0.5dp"
                android:layout_height="@dimen/dimen_5"
                android:background="@color/color_666666"
                android:layout_alignTop="@+id/three_tv_method"
                android:layout_toRightOf="@+id/three_tv_method"
                android:layout_marginLeft="@dimen/dimen_2"
                android:layout_marginTop="@dimen/dimen_2"/>
            <TextView
                android:id="@+id/three_tv_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_666666"
                android:textSize="7sp"
                android:layout_alignTop="@+id/three_tv_method"
                android:layout_toRightOf="@+id/three_view1"
                android:layout_marginLeft="@dimen/dimen_2"/>
            <View
                android:id="@+id/three_view2"
                android:layout_width="0.5dp"
                android:layout_height="@dimen/dimen_5"
                android:background="@color/color_666666"
                android:layout_alignTop="@+id/three_tv_method"
                android:layout_toRightOf="@+id/three_tv_time"
                android:layout_marginLeft="@dimen/dimen_2"
                android:layout_marginTop="@dimen/dimen_2"/>
            <TextView
                android:id="@+id/three_tv_sex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_666666"
                android:textSize="7sp"
                android:layout_alignTop="@+id/three_tv_method"
                android:layout_toRightOf="@+id/three_view2"
                android:layout_marginLeft="@dimen/dimen_2"/>
            <ImageView
                android:id="@+id/three_iv_score"
                android:layout_width="22dp"
                android:layout_height="13dp"
                android:layout_below="@+id/three_tv_method"
                android:layout_marginTop="17dp"
                android:src="@drawable/icon_choice_three"/>
            <TextView
                android:id="@+id/three_tv_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_8"
                android:textColor="@color/color_666666"
                android:gravity="right"
                android:layout_toRightOf="@+id/three_iv_score"
                android:layout_marginLeft="@dimen/dimen_3"
                android:layout_alignBottom="@+id/three_iv_score"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>
        </RelativeLayout>
        <com.part.jianzhiyi.corecommon.ui.ListViewInScrollView
            android:id="@+id/lv_rank"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@color/color_F4F4F4"
            android:dividerHeight="@dimen/line_height"
            android:layout_marginTop="@dimen/dimen_10"
            android:focusable="false"
            android:listSelector="@color/transparency"
            android:scrollbars="none"
            android:paddingBottom="@dimen/dimen_10"
            android:paddingLeft="@dimen/dimen_15"
            android:paddingRight="@dimen/dimen_15"/>
    </LinearLayout>
</ScrollView>