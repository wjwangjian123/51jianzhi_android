                    android:textColor="@color/color_ffffff"
                    android:layout_marginTop="@dimen/dimen_8"
                    android:visibility="gone"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/iv_four_high"
                android:layout_width="21dp"
                android:layout_height="@dimen/dimen_12"
                android:src="@drawable/icon_jifen_high"
                android:layout_toRightOf="@+id/sign_ll_four"
                android:layout_marginLeft="-14dp"
                android:visibility="gone"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/sign_linear2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_10"
            android:gravity="center_horizontal"
            android:layout_gravity="center_horizontal"
            android:layout_below="@+id/sign_linear1"
            android:layout_alignLeft="@+id/sign_linear1">
            <LinearLayout
                android:id="@+id/sign_ll_five"
                android:layout_width="@dimen/dimen_60"
                android:layout_height="73dp"
                android:gravity="center"
                android:orientation="vertical"
                android:background="@drawable/selector_jifen_sign_bg">
                <TextView
                    android:id="@+id/tv_five"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/font_12"
                    android:textColor="@color/color_292E3B"
                    android:layout_marginTop="@dimen/dimen_4"/>
                <ImageView
                    android:id="@+id/iv_five"
                    android:layout_width="@dimen/dimen_30"
                    android:layout_height="@dimen/dimen_30"
                    android:src="@drawable/icon_jifen_zhi1"
                    android:layout_marginTop="@dimen/dimen_1"
                    android:padding="@dimen/dimen_3"/>
                <TextView
                    android:id="@+id/tv_five_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/font_12"
