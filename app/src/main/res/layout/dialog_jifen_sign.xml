                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/font_12"
                    android:textColor="@color/color_A1A7B7"
                    android:layout_marginTop="@dimen/dimen_1"/>
                <ImageView
                    android:id="@+id/iv_two_select"
                    android:layout_width="23dp"
                    android:layout_height="23dp"
                    android:src="@drawable/icon_jifen_dui"
                    android:visibility="gone"/>
                <TextView
                    android:id="@+id/tv_two_select"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="已领取"
                    android:textSize="11sp"
                    android:textColor="@color/color_ffffff"
                    android:layout_marginTop="@dimen/dimen_8"
                    android:visibility="gone"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/iv_two_high"
                android:layout_width="21dp"
                android:layout_height="@dimen/dimen_12"
                android:src="@drawable/icon_jifen_high"
                android:layout_toRightOf="@+id/sign_ll_two"
                android:layout_marginLeft="-14dp"
                android:visibility="gone"/>
            <LinearLayout
                android:id="@+id/sign_ll_three"
                android:layout_width="@dimen/dimen_60"
                android:layout_height="73dp"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/dimen_10"
                android:layout_toRightOf="@+id/sign_ll_two"
                android:background="@drawable/selector_jifen_sign_bg">
                <TextView
                    android:id="@+id/tv_three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/font_12"
                    android:textColor="@color/color_292E3B"
                    android:layout_marginTop="@dimen/dimen_4"/>
                <ImageView
                    android:id="@+id/iv_three"
                    android:layout_width="@dimen/dimen_30"
                    android:layout_height="@dimen/dimen_30"
                    android:src="@drawable/icon_jifen_zhi"
                    android:layout_marginTop="@dimen/dimen_1"/>
                <TextView
                    android:id="@+id/tv_three_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/font_12"
                    android:textColor="@color/color_A1A7B7"
                    android:layout_marginTop="@dimen/dimen_1"/>
                <ImageView
                    android:id="@+id/iv_three_select"
                    android:layout_width="23dp"
                    android:layout_height="23dp"
                    android:src="@drawable/icon_jifen_dui"
                    android:visibility="gone"/>
                <TextView
                    android:id="@+id/tv_three_select"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="已领取"
                    android:textSize="11sp"
                    android:textColor="@color/color_ffffff"
                    android:layout_marginTop="@dimen/dimen_8"
                    android:visibility="gone"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/iv_three_high"
                android:layout_width="21dp"
                android:layout_height="@dimen/dimen_12"
                android:src="@drawable/icon_jifen_high"
                android:layout_toRightOf="@+id/sign_ll_three"
                android:layout_marginLeft="-14dp"
                android:visibility="gone"/>
            <LinearLayout
                android:id="@+id/sign_ll_four"
                android:layout_width="@dimen/dimen_60"
                android:layout_height="73dp"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/dimen_10"
                android:layout_toRightOf="@+id/sign_ll_three"
                android:background="@drawable/selector_jifen_sign_bg">
                <TextView
                    android:id="@+id/tv_four"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/font_12"
                    android:textColor="@color/color_292E3B"
                    android:layout_marginTop="@dimen/dimen_4"/>
                <ImageView
                    android:id="@+id/iv_four"
                    android:layout_width="@dimen/dimen_30"
                    android:layout_height="@dimen/dimen_30"
                    android:src="@drawable/icon_jifen_zhi"
                    android:layout_marginTop="@dimen/dimen_1"/>
                <TextView
                    android:id="@+id/tv_four_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
