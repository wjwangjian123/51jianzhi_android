<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_base"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_ffffff"
    android:orientation="vertical">

    <com.part.jianzhiyi.corecommon.ui.CustomToolbarView
        android:id="@+id/custom_toolbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_44" />

<!--    <include layout="@layout/layout_base_line"/>-->

    <FrameLayout
        android:id="@+id/fl_activity_child_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.part.jianzhiyi.corecommon.customview.LoadDataTipView
            android:id="@+id/rl_load"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </FrameLayout>

</LinearLayout>
